*color15:                         #f8f8f8

Xcursor.theme: xcursor-breeze
Xcursor.size:                     0

/* URxvt.font:                      xft:Source\ Code\ Pro\ for\ Powerline:style      = Regular */
/* URxvt.bold.font:                      xft:Source\ Code\ Pro\ for\ Powerline:style = Bold */
/* URxvt.font: xft:TerminessTTF\ Nerd\ Font\ Mono:style                              = Medium */
URxvt.font:                       xft:Hack\ Nerd\ Font\ Mono:style=Regular:size=10
URxvt.bold.font:                  xft:Hack\ Nerd\ Font\ Mono:style=Bold:size=10
URxvt*italicFont:                 xft:Hack\ Nerd\ Font\ Mono:style=Italic:size=10
URxvt*boldItalicFont:             xft:Hack\ Nerd\ Font\ Mono:style=Bold\ Italic:size=10
! URxvt*boldFont:                   xft:Hack\ Nerd\ Font\ Mono:style                 = Regular
/* URxvt.bold.font:                      xft:TerminessTTF\ Nerd\ Font\ Mono:style = Bold */
